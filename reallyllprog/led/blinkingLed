#! /usr/bin/vvp
:ivl_version "12.0 (stable)" "(v12_0-dirty)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/ivl/system.vpi";
:vpi_module "/usr/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x5adc4f913750 .scope module, "tb" "tb" 2 1;
 .timescale 0 0;
v0x5adc4f938110_0 .var "clk", 0 0;
v0x5adc4f9381e0_0 .net "led", 0 0, v0x5adc4f938010_0;  1 drivers
S_0x5adc4f9274e0 .scope module, "uut" "blink_led" 2 8, 3 1 0, S_0x5adc4f913750;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 1 "led";
v0x5adc4f9276c0_0 .net "clk", 0 0, v0x5adc4f938110_0;  1 drivers
v0x5adc4f937f30_0 .var "counter", 31 0;
v0x5adc4f938010_0 .var "led", 0 0;
E_0x5adc4f8ede60 .event posedge, v0x5adc4f9276c0_0;
    .scope S_0x5adc4f9274e0;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5adc4f937f30_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x5adc4f9274e0;
T_1 ;
    %wait E_0x5adc4f8ede60;
    %load/vec4 v0x5adc4f937f30_0;
    %cmpi/e 50000000, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5adc4f937f30_0, 0;
    %load/vec4 v0x5adc4f938010_0;
    %inv;
    %assign/vec4 v0x5adc4f938010_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5adc4f937f30_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x5adc4f937f30_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5adc4f913750;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5adc4f938110_0, 0, 1;
T_2.0 ;
    %delay 10, 0;
    %load/vec4 v0x5adc4f938110_0;
    %inv;
    %store/vec4 v0x5adc4f938110_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x5adc4f913750;
T_3 ;
    %vpi_call 2 21 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5adc4f913750 {0 0 0};
    %delay 10000000, 0;
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb.v";
    "main.v";
